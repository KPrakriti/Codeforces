import java.util.*;
public class team{
 
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt(); //take input for number of problems
		int count = 0;
		while(n-->0)
		{
			int a = sc.nextInt();
			int b = sc.nextInt();
			int c = sc.nextInt();
			
			int sum = a+b+c;
			
			if(sum > 1)
			{
				count++;
			}
			
		}
		System.out.println(count);
	}
 
}
